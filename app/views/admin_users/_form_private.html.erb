<%= error_messages_for(@private_pictures) %>

<table summary="Picture form fields">
  <tr>
    <th><%= f.label(:private_user_id) %></th>
    <td><%= f.label(@private_user.name)%>
<%= f.text_field(:private_user_id, :readonly => true, :value => @private_user.id, :hidden=> true) %></td>
  </tr>
  <tr>
    <th><%= f.label(:name) %></th>
    <td><%= f.text_field(:name) %></td>
  </tr>
  <tr>
    <th><%= f.label(:location) %></th>
    <td><%= f.text_field(:location) %></td>
  </tr>
  <tr>
    <th><%= f.label(:visible) %></th>
    <td><%= f.text_field(:visible) %></td>
  </tr>
  <tr>
    <th><%= f.label(:summary) %></th>
    <td><%= f.text_field(:summary) %></td>
  </tr>
  <tr>
    <th><%= f.label(:description) %></th>
    <td><%= f.text_field(:description) %></td>
  </tr>
</table>
